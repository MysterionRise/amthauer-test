<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Group?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>

<Group id="Amthauer Test" fx:id="rootGroup" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.mystic.amthauer.Controller">
    <Pane prefHeight="900.0" prefWidth="800.0">
        <children>
            <Button layoutX="704.0" layoutY="13.0" mnemonicParsing="false" onMouseClicked="#closeWindow" text="Выход" />

            <Button fx:id="startTestButton" layoutX="233.0" layoutY="13.0" mnemonicParsing="false" onMouseClicked="#startTest" text="Старт" />

            <TextArea fx:id="instructionText" editable="false" layoutX="14.0" layoutY="49.0" prefHeight="794.0" prefWidth="745.0" wrapText="true" />
            <TextField fx:id="userName" layoutX="14.0" layoutY="13.0" onMouseClicked="#clearTextField" prefWidth="200.0" text="Введите свое имя" />
            <Button fx:id="nextStep" layoutX="704.0" layoutY="860.0" mnemonicParsing="false" text="Далее" visible="false" onMouseClicked="#nextStep"/>
         <Label fx:id="testName" layoutX="400.0" layoutY="15.0" prefHeight="20.0" prefWidth="200.0" text="" visible="false" />
        </children>
    </Pane>
</Group>
